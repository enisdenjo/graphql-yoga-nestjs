{
  "name": "yoga-nest-code-first",
  "version": "1.0.0",
  "description": "Yoga Nest code first example",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
