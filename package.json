{
  "name": "@graphql-yoga/nestjs",
  "version": "0.3.1",
  "type": "module",
  "description": "GraphQL Yoga driver for NestJS GraphQL.",
  "repository": {
    "type": "git",
    "url": "https://github.com/enisdenjo/graphql-yoga-nestjs.git"
  },
  "author": "Denis Badurina <badurinadenis@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "files": [
    "LICENSE.md",
    "lib",
    "README.md"
  ],
  "keywords": [
    "graphql",
    "server",
    "nestjs",
    "nest",
    "driver",
    "graphql-yoga"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:esm": "tsc -b tsconfig.esm.json && node scripts/esm-post-process.mjs",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint 'src'",
    "lint:prettier": "prettier -c .",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@apollo/gateway": "^2.0.0",
    "@apollo/subgraph": "^2.0.0",
    "@envelop/apollo-federation": "^3.0.0",
    "@nestjs/common": "^8.4.7 || ^9.0.0",
    "@nestjs/core": "^8.4.7 || ^9.0.0",
    "@nestjs/graphql": "^10.0.0",
    "graphql-yoga": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@apollo/gateway": {
      "optional": true
    },
    "@apollo/subgraph": {
      "optional": true
    },
    "@envelop/apollo-federation": {
      "optional": true
    }
  },
  "devDependencies": {
    "@apollo/gateway": "0.54.1",
    "@apollo/subgraph": "0.6.1",
    "@changesets/cli": "2.22.0",
    "@envelop/apollo-federation": "3.0.4",
    "@nestjs/common": "9.2.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/platform-fastify": "9.2.1",
    "@nestjs/testing": "9.2.1",
    "@theguild/eslint-config": "^0.7.0",
    "@theguild/prettier-config": "^1.1.0",
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-ws": "1.0.20",
    "bob-the-bundler": "4.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "8.6.0",
    "fastify": "3.29.5",
    "graphql": "15.8.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-yoga": "^3.6.0",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "lint-staged": "13.1.0",
    "patch-package": "6.5.1",
    "prettier": "^2.8.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.8.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-morph": "15.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
