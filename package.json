{
  "name": "@graphql-yoga/nestjs",
  "version": "0.1.0",
  "description": "Yoga driver for Nest GraphQL",
  "author": "Charly Poly <charly@the-guild.dev>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "graphql",
    "server",
    "nestjs",
    "graphql-yoga"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "scripts": {
    "test": "jest --passWithNoTests --config ./tests/jest-e2e.ts --runInBand",
    "test:dev": "jest --passWithNoTests --config ./tests/jest-e2e.ts --runInBand --watch",
    "prepublishOnly": "yarn build",
    "clean": "rimraf dist",
    "postinstall": "patch-package",
    "prebuild": "yarn clean",
    "build": "bob build --single",
    "prettier": "prettier --write --list-different .",
    "prettier:check": "prettier --check .",
    "lint": "eslint .",
    "prerelease": "yarn build",
    "bob:bootstrap": "bob bootstrap",
    "release": "changeset publish",
    "release:canary": "(node scripts/canary-release.cjs && yarn build && yarn changeset publish --tag canary) || echo Skipping Canary..."
  },
  "dependencies": {
    "@envelop/apollo-server-errors": "^3.3.1",
    "iterall": "1.3.0",
    "graphql-yoga": "3.1.0",
    "@nestjs/graphql": "10.0.16"
  },
  "devDependencies": {
    "patch-package": "6.4.7",
    "rimraf": "3.0.2",
    "@changesets/cli": "2.22.0",
    "bob-the-bundler": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "12.5.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "prettier": "2.6.2",
    "@types/jest": "27.5.2",
    "jest": "28.1.0",
    "supertest": "6.2.3",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0",
    "ts-morph": "15.0.0",
    "typescript": "4.7.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "@apollo/subgraph": "0.4.2",
    "@envelop/apollo-federation": "2.3.2",
    "reflect-metadata": "0.1.13",
    "fastify": "3.29.0",
    "graphql": "15.8.0",
    "graphql-subscriptions": "2.0.0",
    "rxjs": "7.5.5",
    "apollo-client": "2.6.10",
    "apollo-link-ws": "1.0.20",
    "apollo-cache-inmemory": "1.6.6",
    "@apollo/gateway": "0.51.0",
    "@nestjs/common": "8.4.7",
    "@nestjs/core": "8.4.7",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/platform-fastify": "8.4.7",
    "@nestjs/testing": "8.4.7"
  },
  "peerDependencies": {
    "@apollo/gateway": "^0.44.1 || ^0.46.0 || ^0.48.0 || ^0.49.0 || ^0.50.0 || ^0.51.0 || ^2.0.0",
    "@nestjs/common": "8.4.7",
    "@nestjs/core": "8.4.7"
  },
  "peerDependenciesMeta": {
    "@apollo/gateway": {
      "optional": true
    },
    "@envelop/apollo-federation": {
      "optional": true
    },
    "@apollo/subgraph": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlypoly/graphql-yoga-nestjs"
  },
  "lint-staged": {
    "{src,test}/**/*": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "type": "module",
  "typings": "dist/typings/index.d.ts"
}
