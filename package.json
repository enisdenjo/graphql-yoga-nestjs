{
  "name": "@graphql-yoga/nestjs",
  "version": "0.3.1",
  "type": "module",
  "description": "GraphQL Yoga driver for NestJS GraphQL.",
  "repository": {
    "type": "git",
    "url": "https://github.com/enisdenjo/graphql-yoga-nestjs.git"
  },
  "author": "Denis Badurina <badurinadenis@gmail.com>",
  "license": "MIT",
  "packageManager": "yarn@3.4.1",
  "engines": {
    "node": ">=14"
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "files": [
    "LICENSE.md",
    "lib",
    "README.md"
  ],
  "keywords": [
    "graphql",
    "server",
    "nestjs",
    "nest",
    "driver",
    "graphql-yoga"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:esm": "tsc -b tsconfig.esm.json && node scripts/esm-post-process.mjs",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint 'src'",
    "lint:prettier": "prettier -c .",
    "test": "vitest tests",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@apollo/gateway": "^2.0.0",
    "@apollo/subgraph": "^2.0.0",
    "@envelop/apollo-federation": "^3.0.0",
    "@nestjs/common": "^8.4.7 || ^9.0.0",
    "@nestjs/core": "^8.4.7 || ^9.0.0",
    "@nestjs/graphql": "^10.0.0",
    "graphql-yoga": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@apollo/gateway": {
      "optional": true
    },
    "@apollo/subgraph": {
      "optional": true
    },
    "@envelop/apollo-federation": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/testing": "^9.3.9",
    "@theguild/eslint-config": "^0.7.0",
    "@theguild/prettier-config": "^1.1.0",
    "@types/eslint": "^8.21.1",
    "eslint": "^8.32.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.6.0",
    "prettier": "^2.8.3",
    "typescript": "4.9.4",
    "vitest": "^0.28.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
